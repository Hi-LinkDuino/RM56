<div class="container">
    <div class="sub-container">
        <div class="style-container">
            <text class="title">
                通用样式
            </text>
            <text class="sub-title">
                select通用样式1
            </text>
            <select class="style1" id="style1">
            </select>
            <text  class="sub-title">
                select通用样式2
            </text>
            <select class="style2" id="style2">
            </select>
            <text class="sub-title">
                select通用样式3
            </text>
            <select class="style3" id="style3">
            </select>
            <text  class="sub-title">
                select通用样式4
            </text>
            <select class="style4" id="style4">
            </select>
            <text  class="sub-title">
                select通用样式5
            </text>
            <div class="contain1">
                <select class="style5" id="style5">
                </select>
                <select class="style6" id="style6">
                </select>
            </div>
            <text  class="sub-title">
                select特有样式1
            </text>
            <div class="style7" id="style7">
                <select class="flex-item font-family">
                    <option value="1">1</option>
                    <option value="2">2</option>
                </select>
            </div>
        </div>
        <divider style="color: #000000; stroke-width: 2px;" vertical="false">
        </divider>
        <div class="ani-container">
            <text class="title">
                动画样式
            </text>
            <text class="sub-title">
                select动画样式1
            </text>
            <select class="ani1" >
            </select>
            <text class="sub-title">
                select动画样式2
            </text>
            <select class="ani2" >
            </select>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                select通用属性1
            </text>
            <select id="prop1" class="prop1"
                 disabled = "true" focusable ="true"
                 data-name ="prop1" click-effect="spring-medium"
                 dir ="rtl">
            </select>
            <text  class="sub-title">
                select通用属性2
            </text>
            <select class="prop2" ref ="prop2" id="prop2"
                 disabled = "false" focusable ="false"
                 data-name ="prop2" click-effect="spring-large"
                 dir ="ltr">
            </select>
            <text  class="sub-title">
                select通用属性3
            </text>
            <select style="color: #679855; margin: 5px;" class="prop3" id="prop3">
            </select>
            <text  class="sub-title">
                select特有属性1
            </text>
            <select if="false" class="prop2" ref ="prop2" style="background-color: rebeccapurple;">
            </select>
            <select elif="true" class="prop2" ref ="prop2" style="background-color: black;">
            </select>
            <select elif="false" class="prop2" ref ="prop2" style="background-color: blue;">
            </select>
            <select else class="prop2" ref ="prop2" style="background-color: yellow;">
            </select>
            <select for="{{array}}" tid="id" style="color: #679855; margin: 1px;" class="prop3">
            </select>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>
            <text  class="sub-title">
                select通用事件1
            </text>
            <select class ="event1" ontouchstart="touchStart" ontouchmove="touchMove"
                 ontouchend="touchEnd" ontouchcancel="touchCancel">
            </select>
            <text  class="sub-title">
                select通用事件2
            </text>
            <select class ="event2" onclick="click" ondoubleclick="doubleClick"
                 onlongpress="longPress" onfocus="focus" onblur="blur" onkey="key"
                 onswipe="swipe" onattached="attached" ondetached="detached">
            </select>
            <text  class="sub-title">
                select通用事件3
            </text>
            <select class ="event3" onpinchstart="pinchStart" onpinchupdate="pinchUpdate"
                 onpinchend="pinchEnd" onpinchcancel="pinchCancel"
                 ondragstart="dragStart" ondrag="drag"
                 ondragend="dragEnd" ondragenter="dragEnter"
                 ondragover="dragOver" ondragleave="dragLeave"
                 ondrop="drop" >
            </select>
            <text  class="sub-title">
                select特有事件1
            </text>
            <select class ="event4"
                   onchange="change"
                    onaccessibility="onAccessibility">
            </select>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="function-container">
            <text class="title">
                通用方法
            </text>
            <text class="sub-title">
                select通用方法1
            </text>
            <select id="function1" class="function1" ontouchstart="functionTest1">
            </select>
            <text class="sub-title">
                select通用方法2
            </text>
            <select id="function2" class="function2" ontouchstart="functionTest2">
            </select>
            <text class="sub-title">
                select通用方法3
            </text>
            <select id="function3" class="function3" ontouchstart="functionTest3">
            </select>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
        <div class="gradient-container">
            <text class="title">
                渐变样式
            </text>
            <text class="sub-title">
                select渐变样式1
            </text>
            <select class="gradient1">
            </select>
            <text  class="sub-title">
                select渐变样式2
            </text>
            <select class="gradient2">
            </select>
            <text  class="sub-title">
                select渐变样式3
            </text>
            <select class="gradient3">
            </select>
            <text  class="sub-title">
                select渐变样式4
            </text>
            <select class="gradient4">
            </select>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="access-container">
            <text class="title">
                无障碍
            </text>
            <text class="sub-title">
                select无障碍1
            </text>
            <select class="access1" accessibilitygroup ="true"
                 accessibilitytext="这是select"
                 accessibilitydescription="点击此按键会弹出一个对话框"
                 accessibilityimportance="no-hide-descendants">
            </select>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="atom-container">
            <text class="title">
                原子布局
            </text>
            <text class="sub-title">
                select原子布局1
            </text>
            <div style="flex-direction: row;height: 30px;width: 100%;">
                <select style="width:20px;height:20px;background-color: blue;display-index: 1;">
                </select>
                <select style="width:30px; height:20px;background-color: black;display-index: 5;">
                </select>
                <select style="width:40px; height:20px;background-color: yellow;display-index: 4;">
                </select>
                <select style="width:10px; height:20px;background-color: red;display-index: 3;">
                </select>
                <select style="width:25px; height:20px;background-color:pink;display-index: 7;">
                </select>
                <select style="width:15px; height:20px;background-color: palegoldenrod;display-index: 2;">
                </select>
            </div>
            <text class="sub-title">
                select原子布局2
            </text>
            <div style="flex-direction: row;height: 30px;width: 100%;">
                <select style="height:20px;background-color: blue;flex-weight: 1;">
                </select>
                <select style="height:20px;background-color: black;flex-weight: 5;">
                </select>
                <select style="height:20px;background-color: yellow;flex-weight: 4;">
                </select>
                <select style="height:20px;background-color: red;flex-weight: 3;">
                </select>
                <select style="height:20px;background-color:pink;flex-weight: 7;">
                </select>
                <select style="height:20px;background-color: palegoldenrod;flex-weight: 2;">
                </select>
            </div>
            <text class="sub-title">
                select原子布局3
            </text>
            <div style="flex-direction: row;height:60px;width: 100%;">
                <select style="width:30px;background-color: blue;aspect-ratio: 0.6;">
                </select>
                <select style="width:30px;background-color: black;aspect-ratio:0.5;">
                </select>
                <select style="width:30px;background-color: yellow;aspect-ratio: 1.5;">
                </select>
                <select style="width:30px;background-color: red;aspect-ratio: 1.3;">
                </select>
                <select style="width:30px;background-color:pink;aspect-ratio: 1;">
                </select>
                <select style="width:30px;background-color: palegoldenrod;aspect-ratio:2;">
                </select>
            </div>
            <div style="background-color: #000000; width: 100%; height: 2px;">
            </div>
            <div class="multiMode-container">
                <text class="title">
                    多模输入
                </text>
                <text class="sub-title">
                    select多模输入1
                </text>
                <select class="multiMode1" voicelabel = "voice"
                     subscriptflag="on" subscriptlabel="select" scenelabel="common">
                </select>
            </div>
        </div>
    </div>
</div>
