<div class="container">
    <div class="sub-container">
    <!-- 通用样式 -->
        <div class="style-container">
            <text class="title">
                通用样式
            </text>

            <text class="sub-title">
                switch通用样式1
            </text>
            <switch id="style1" class="style1">
            </switch>

            <text  class="sub-title">
                switch通用样式2
            </text>
            <switch id="style2" class="style2"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text  class="sub-title">
                switch通用样式3
            </text>
            <switch id="style3" class="style3"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text  class="sub-title">
                switch通用样式4
            </text>
            <switch id="style4" class="style4"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text  class="sub-title">
                switch通用样式5
            </text>
            <div class="contain1">
                <switch id="style5" class="style5"
                        showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
                </switch>
                <switch id="style6" class="style6"
                        showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
                </switch>
            </div>

            <text  class="sub-title">
                switch特有样式
            </text>
            <switch id="style7" class="style7"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text  class="sub-title">
                switch特有样式
            </text>
            <switch id="style8" class="style8"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

        </div>

        <divider style="color: #000000; stroke-width: 2px;" vertical="false">
        </divider>
    <!-- 动画 -->
        <div class="ani-container">
            <text class="title">
                动画样式
            </text>

            <text class="sub-title">
                switch动画样式1
            </text>
            <switch id="ani1" class="ani1"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text class="sub-title">
                switch动画样式2
            </text>
            <switch id="ani2" class="ani2"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 通用属性 -->
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                switch通用属性1
            </text>
            <switch  id="prop1"
                   class="prop1"
                   disabled = "true"
                   focusable ="true"
                   data-name ="prop1"
                   click-effect="spring-medium"
                   dir ="rtl">
            </switch>

            <text  class="sub-title">
                switch通用属性2
            </text>
            <switch  id="prop2"
                   class="prop2"
                   ref ="prop2"
                   disabled = "false"
                   focusable ="false"
                   data-name ="prop2"
                   click-effect="spring-large"
                   dir ="ltr">
            </switch>

            <text  class="sub-title">
                switch通用属性3
            </text>
            <switch id="prop3" style="color: #679855; margin: 5px;width: 100%;height: 40px;">
            </switch>

            <text  class="sub-title">
                switch特有属性4
            </text>
            <switch id="prop4" class="prop4"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text  class="sub-title">
                switch特有属性5
            </text>
            <switch id="prop5" class="prop5"
                    showtext="false" texton="打开" textoff="关掉" checked="false" @change="switchChange">
            </switch>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用事件 -->
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>

            <text  class="sub-title">
                switch通用事件1
            </text>
            <switch id="event1" class ="event1" ontouchstart="touchStart" ontouchmove="touchMove"
                   ontouchend="touchEnd" ontouchcancel="touchCancel">
            </switch>

            <text  class="sub-title">
                switch通用事件2
            </text>
            <switch id="event2" class="event2" onclick="click" ondoubleclick="doubleClick"
                   onlongpress="longPress" onfocus="focus" onblur="blur" onkey="key"
                   onswipe="swipe" onattached="attached" ondetached="detached" onaccessibility="attached">
            </switch>

            <text  class="sub-title">
                switch通用事件3
            </text>
            <switch id="event3" class="event3" onpinchstart="pinchStart" onpinchupdate="pinchUpdate"
                   onpinchend="pinchEnd" onpinchcancel="pinchCancel"
                   ondragstart="dragStart" ondrag="drag"
                   ondragend="dragEnd" ondragenter="dragEnter"
                   ondragover="dragOver" ondragleave="dragLeave"
                   ondrop="drop">
            </switch>

            <text  class="sub-title">
                switch特有事件1
            </text>
            <switch id="event4" class="event4"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>

            <text  class="sub-title">
                switch特有事件2
            </text>
            <switch id="event5" class="event5"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用方法 -->
        <div class="function-container">
            <text class="title">
                通用方法
            </text>

            <text class="sub-title">
                switch通用方法1
            </text>
            <switch id="function1" class="function1" ontouchstart="functionTest1">
            </switch>

            <text class="sub-title">
                switch通用方法2
            </text>
            <switch id="function2" class="function2" ontouchstart="functionTest2">
            </switch>

            <text class="sub-title">
                switch通用方法3
            </text>
            <switch id="function3" class="function3" ontouchstart="functionTest3">
            </switch>
            
            <text class="sub-title">
                switch特有方法1
            </text>
            <switch id="function4"
                    showtext="true" texton="开启" textoff="关闭" checked="true" @change="switchChange">
            </switch>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 渐变样式 -->
        <div class="gradient-container">
            <text class="title">
                渐变样式
            </text>

            <text class="sub-title">
                switch渐变样式1
            </text>
            <switch id="gradient1" class="gradient1">
            </switch>

            <text  class="sub-title">
                switch渐变样式2
            </text>
            <switch id="gradient2" class="gradient2">
            </switch>

            <text  class="sub-title">
                switch渐变样式3
            </text>
            <switch id="gradient3" class="gradient3">
            </switch>

            <text  class="sub-title">
                switch渐变样式4
            </text>
            <switch id="gradient4" class="gradient4">
            </switch>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 无障碍 -->
        <div class="access-container">
            <text class="title">
                无障碍
            </text>
            <text class="sub-title">
                switch无障碍1
            </text>
            <switch id="access1" class="access1"
                   accessibilitygroup ="true"
                   accessibilitytext="这是div"
                   accessibilitydescription="点击此按键会弹出一个对话框"
                   accessibilityimportance="no-hide-descendants"
                   >
            </switch>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 原子布局和多模输入 -->
        <div class="atom-container">
            <text class="title">
                原子布局
            </text>
            <text class="sub-title">
                switch原子布局1
            </text>
            <div style="flex-direction: row;height: 40px;width: 100%;">
                <switch id="atomA1" class="atom1">
                </switch>
                <switch id="atomA2" class="atom2">
                </switch>
                <switch id="atomA3" class="atom3">
                </switch>
            </div>
            <text class="sub-title">
                switch原子布局2
            </text>
            <div style="flex-direction: row;height: 40px;width: 100%;">
                <switch id="atomB1" class="atom1">
                </switch>
                <switch id="atomB2" class="atom2">
                </switch>
                <switch id="atomB3" class="atom3">
                </switch>
            </div>
            <text class="sub-title">
                switch原子布局3
            </text>
            <div style="flex-direction: row;height:40px;width: 100%;">
                <switch id="atomC1" class="atom1">
                </switch>
                <switch id="atomC2" class="atom2">
                </switch>
                <switch id="atomC3" class="atom3">
                </switch>
            </div>
            <div style="background-color: #000000; width: 100%; height: 2px;">
            </div>
            <div class="multimode-container">
                <text class="title">
                    多模输入
                </text>
                <text class="sub-title">
                    switch多模输入1
                </text>
                <switch id="multiMode1"
                       class="multimode1"
                       voicelabel = "voice"
                       subscriptflag="on"
                       subscriptlabel="div"
                       scenelabel="common">
                </switch>
            </div>
        </div>
    </div>
</div>






