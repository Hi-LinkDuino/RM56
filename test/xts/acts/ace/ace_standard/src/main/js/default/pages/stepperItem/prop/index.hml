<div class="container">
    <div class="prop-container">
        <text class="title">
            stepper通用属性
        </text>

        <text class="sub-title">
            通用属性 -- id
        </text>
        <stepper class="stepper-style">
            <stepper-item id="idProp">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- class
        </text>
        <stepper class="stepper-style">
            <stepper-item id="classProp"
                          class="classProp">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="classPropNone"
                          class="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- style
        </text>
        <stepper class="stepper-style">
            <stepper-item id="styleProp"
                          style="width:10%;height:20px;background-color:red">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- ref
        </text>
        <stepper class="stepper-style">
            <stepper-item id="refProp"
                          ref="refProp">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="refPropNone"
                          ref="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- disabled
        </text>
        <stepper class="stepper-style">
            <stepper-item id="disabledPropTrue"
                          disabled="true">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="disabledPropFalse"
                          disabled="false">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="disabledPropNone"
                          disabled="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- focusable
        </text>
        <stepper class="stepper-style">
            <stepper-item id="focusablePropTrue"
                          focusable="true">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="focusablePropFalse"
                          focusable="false">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="focusablePropNone"
                          focusable="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- data-*
        </text>
        <stepper class="stepper-style">
            <stepper-item id="dataProp"
                          data-div="通用属性 -- data-*">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="dataPropNone"
                          data-div="">
            </stepper-item>
        </stepper>

    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="specific-container">
        <text class="sub-title">
            通用属性 -- click-effect
        </text>
        <stepper class="stepper-style">
            <stepper-item id="clickEffectPropSmall"
                          click-effect="spring-small">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="clickEffectPropMedium"
                          click-effect="spring-medium">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="clickEffectPropLarge"
                          click-effect="spring-large">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="clickEffectPropNone"
                          click-effect="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            通用属性 -- dir
        </text>
        <stepper class="stepper-style">
            <stepper-item id="dirPropRtl"
                          dir="rtl">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="dirPropAuto"
                          dir="auto">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="dirPropLtr"
                          dir="ltr">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="dirPropNone"
                          dir="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            渲染属性 -- for
        </text>
        <stepper class="stepper-style">
            <stepper-item id="forPropNull"
                          for="">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="forPropOne"
                          for="{{listOne}}">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="forPropThree"
                          for="{{listThree}}">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            渲染属性 -- if
        </text>
        <stepper class="stepper-style">
            <stepper-item id="ifPropTrue"
                          if="true">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            渲染属性 -- show
        </text>
        <stepper class="stepper-style">
            <stepper-item id="showPropTrue"
                          show="true">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="showPropFalse"
                          show="false">
            </stepper-item>
        </stepper>
        <stepper class="stepper-style">
            <stepper-item id="showPropNone"
                          show="">
            </stepper-item>
        </stepper>

        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>

        <text class="title">
            stepper特有属性
        </text>

        <text class="sub-title">
            特有属性 -- stepper-item
        </text>
        <stepper class="stepper-style">
            <stepper-item id="stepperItemProp"
                          stepper-item="stepper-item">
            </stepper-item>
            <stepper-item id="stepperItemPropNone"
                          stepper-item="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            特有属性 -- tid
        </text>
        <stepper class="stepper-style">
            <stepper-item id="tidProp"
                          tid="tid">
            </stepper-item>
            <stepper-item id="tidPropNone"
                          tid="">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            特有属性 -- elif
        </text>
        <stepper class="stepper-style">
            <stepper-item if="false"></stepper-item>
            <stepper-item id="elifPropTrue"
                          elif="true">
            </stepper-item>
            <stepper-item id="tidPropNone"
                          elif="false">
            </stepper-item>
        </stepper>

        <text class="sub-title">
            底部按钮属性 -- label
        </text>
        <stepper class="stepper-style">
            <stepper-item id="labelProp"
                          label="{{ labelObj }}">
            </stepper-item>
        </stepper>
    </div>
</div>
