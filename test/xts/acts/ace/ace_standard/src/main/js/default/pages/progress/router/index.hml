<div class="container">
    <div class="sub-container">
    <!-- 通用样式 -->
        <div class="style-container">
            <text class="title">
                通用样式
            </text>

            <text class="sub-title">
                progress通用样式1
            </text>
            <progress percent="10" id="style1" class="style1">
            </progress>

            <text  class="sub-title">
                progress通用样式2
            </text>
            <progress percent="10" id="style2" class="style2">
            </progress>

            <text  class="sub-title">
                progress通用样式3
            </text>
            <progress percent="10" id="style3" class="style3">
            </progress>

            <text  class="sub-title">
                progress通用样式4
            </text>
            <progress percent="10" id="style4" class="style4">
            </progress>

            <text  class="sub-title">
                progress通用样式5
            </text>
            <div class="contain1">
                <progress percent="10" id="style5" class="style5">
                </progress>
                <progress percent="10" id="style6" class="style6">
                </progress>
            </div>

            <text  class="sub-title">
                progress特有样式
            </text>
            <progress percent="10" secondarypercent="50" id="style7" class="style7">
            </progress>
        </div>

        <divider style="color: #000000; stroke-width: 2px;" vertical="false">
        </divider>
    <!-- 动画 -->
        <div class="ani-container">
            <text class="title">
                动画样式
            </text>

            <text class="sub-title">
                progress动画样式1
            </text>
            <progress percent="10" id="ani1" class="ani1">
            </progress>

            <text class="sub-title">
                progress动画样式2
            </text>
            <progress percent="10" id="ani2" class="ani2">
            </progress>

        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 通用属性 -->
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                progress通用属性1
            </text>
            <progress percent="10"  id="prop1"
                   class="prop1"
                   disabled = "true"
                   focusable ="true"
                   data-name ="prop1"
                   click-effect="spring-medium"
                   dir ="rtl">
            </progress>

            <text  class="sub-title">
                progress通用属性2
            </text>
            <progress percent="10" class="prop2"
                   ref ="prop2"
                   disabled = "false"
                   focusable ="false"
                   data-name ="prop2"
                   click-effect="spring-large"
                   dir ="ltr">
            </progress>

            <text  class="sub-title">
                progress通用属性3
            </text>
            <progress percent="10" id="prop3" style="color: #679855; margin: 5px;width: 100%;height: 40px;" class="prop3" >
            </progress>

            <text  class="sub-title">
                progress特有属性4
            </text>
            <progress percent="10" id="prop4" class="prop4" type="circular">
            </progress>

            <text  class="sub-title">
                progress特有属性5
            </text>
            <progress percent="10" id="prop5" class="prop5" type="ring">
            </progress>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用事件 -->
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>
            <text  class="sub-title">
                progress通用事件1
            </text>
            <progress percent="10" id="event1" class ="event1" ontouchstart="touchStart" ontouchmove="touchMove"
                   ontouchend="touchEnd" ontouchcancel="touchCancel">
            </progress>

            <text  class="sub-title">
                progress通用事件2
            </text>
            <progress percent="10" id="event2" class="event2" onclick="click" ondoubleclick="doubleClick"
                   onlongpress="longPress" onfocus="focus" onblur="blur" onkey="key"
                   onaccessibility="onAccessibility"
                   onswipe="swipe" onattached="attached" ondetached="detached">
            </progress>

            <text  class="sub-title">
                progress通用事件3
            </text>
            <progress percent="10" id="event3" class="event3" onpinchstart="pinchStart" onpinchupdate="pinchUpdate"
                   onpinchend="pinchEnd" onpinchcancel="pinchCancel"
                   ondragstart="dragStart" ondrag="drag"
                   ondragend="dragEnd" ondragenter="dragEnter"
                   ondragover="dragOver" ondragleave="dragLeave"
                   ondrop="drop">
            </progress>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用方法 -->
        <div class="function-container">
            <text class="title">
                通用方法
            </text>
            <text class="sub-title">
                progress通用方法1
            </text>
            <progress percent="10" id="function1" class="function1" ontouchstart="functionTest1">
            </progress>


            <text class="sub-title">
                progress通用方法2
            </text>
            <progress percent="10" id="function2" class="function2" ontouchstart="functionTest2">
            </progress>


            <text class="sub-title">
                progress通用方法3
            </text>
            <progress percent="10" id="function3" class="function3" ontouchstart="functionTest3">
            </progress>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 渐变样式 -->
        <div class="gradient-container">
            <text class="title">
                渐变样式
            </text>

            <text class="sub-title">
                progress渐变样式1
            </text>
            <progress percent="10" id="gradient1" class="gradient1">
            </progress>

            <text  class="sub-title">
                progress渐变样式2
            </text>
            <progress percent="10" id="gradient2" class="gradient2">
            </progress>

            <text  class="sub-title">
                progress渐变样式3
            </text>
            <progress percent="10" id="gradient3" class="gradient3">
            </progress>

            <text  class="sub-title">
                progress渐变样式4
            </text>
            <progress percent="10" id="gradient4" class="gradient4">
            </progress>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 无障碍 -->
        <div class="access-container">
            <text class="title">
                无障碍
            </text>
            <text class="sub-title">
                progress无障碍1
            </text>
            <progress percent="10" class="access1" accessibilitygroup ="true"
                 accessibilitytext="这是div"
                 accessibilitydescription="点击此按键会弹出一个对话框"
                 accessibilityimportance="no-hide-descendants">
            </progress>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="atom-container">
            <text class="title">
                原子布局
            </text>
            <text class="sub-title">
                progress原子布局1
            </text>
            <div style="flex-direction: row;height: 30px;width: 100%;">
                <div style="width:20px;height:20px;background-color: blue;display-index: 1;">
                </div>
                <div style="width:30px; height:20px;background-color: black;display-index: 5;">
                </div>
                <div style="width:40px; height:20px;background-color: yellow;display-index: 4;">
                </div>
                <div style="width:10px; height:20px;background-color: red;display-index: 3;">
                </div>
                <div style="width:25px; height:20px;background-color:pink;display-index: 7;">
                </div>
                <div style="width:15px; height:20px;background-color: palegoldenrod;display-index: 2;">
                </div>
            </div>
            <text class="sub-title">
                progress原子布局2
            </text>
            <div style="flex-direction: row;height: 30px;width: 100%;">
                <progress percent="10" style="height:20px;background-color: blue;flex-weight: 1;">
                </progress>
                <progress percent="10" style="height:20px;background-color: black;flex-weight: 5;">
                </progress>
                <progress percent="10" style="height:20px;background-color: yellow;flex-weight: 4;">
                </progress>
                <progress percent="10" style="height:20px;background-color: red;flex-weight: 3;">
                </progress>
                <progress percent="10" style="height:20px;background-color:pink;flex-weight: 7;">
                </progress>
                <progress percent="10" style="height:20px;background-color: palegoldenrod;flex-weight: 2;">
                </progress>
            </div>
            <text class="sub-title">
                progress原子布局3
            </text>
            <div style="flex-direction: row;height:60px;width: 100%;">
                <progress percent="10" style="width:30px;background-color: blue;aspect-ratio: 0.6;">
                </progress>
                <progress percent="10" style="width:30px;background-color: black;aspect-ratio:0.5;">
                </progress>
                <progress percent="10" style="width:30px;background-color: yellow;aspect-ratio: 1.5;">
                </progress>
                <progress percent="10" style="width:30px;background-color: red;aspect-ratio: 1.3;">
                </progress>
                <progress percent="10" style="width:30px;background-color:pink;aspect-ratio: 1;">
                </progress>
                <progress percent="10" style="width:30px;background-color: palegoldenrod;aspect-ratio:2;">
                </progress>
            </div>
            <div style="background-color: #000000; width: 100%; height: 2px;">
            </div>
            <div class="multimode-container">
                <text class="title">
                    多模输入
                </text>
                <text class="sub-title">
                    progress多模输入1
                </text>
                <progress percent="10" id="multiMode1"
                      class="multiMode1"
                      percent="10"
                      voicelabel = "voice"
                      subscriptflag="on"
                      subscriptlabel="div"
                      scenelabel="common">
                </progress>
            </div>
        </div>
    </div>
</div>






