<div class="container">
    <div class="sub-container">
        <div class="style-container">
            <text class="title">
                通用样式
            </text>
            <text class="sub-title">
                richtext通用样式1
            </text>
            <div class="yellowgreenDiv">
                <richtext class="style1" id="style1">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext通用样式2
            </text>
            <div class="greenyellowDiv">
                <richtext class="style2" id="style2">
                </richtext>
            </div>
        </div>
        <divider style="color: #000000; stroke-width: 2px;" vertical="false">
        </divider>
        <div class="ani-container">
            <text class="title">
                动画样式
            </text>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                richtext通用属性1
            </text>
            <div class="greenyellowDiv">
                <richtext id="prop1">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext通用属性2
            </text>
            <div class="yellowDiv">
                <richtext id="prop2" style="visibility: visible;">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext通用属性3
            </text>
            <div class="greenyellowDiv">
                <richtext id="prop3" class="prop3">
                </richtext>
            </div>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>
            <text  class="sub-title">
                richtext通用事件1
            </text>
            <div class="greenyellowDiv">
                <richtext class ="event1" ontouchstart="touchStart" ontouchmove="touchMove"
                     ontouchend="touchEnd" ontouchcancel="touchCancel">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext通用事件2
            </text>
            <div class="yellowDiv">
                <richtext class ="event2" onclick="click" ondoubleclick="doubleClick"
                     onlongpress="longPress" onfocus="focus" onblur="blur" onkey="key"
                     onswipe="swipe" onattached="attached" ondetached="detached">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext通用事件3
            </text>
            <div class="greenyellowDiv">
                <richtext class ="event3" onpinchstart="pinchStart" onpinchupdate="pinchUpdate"
                     onpinchend="pinchEnd" onpinchcancel="pinchCancel"
                     ondragstart="dragStart" ondrag="drag"
                     ondragend="dragEnd" ondragenter="dragEnter"
                     ondragover="dragOver" ondragleave="dragLeave"
                     ondrop="drop">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext特有事件1
            </text>
            <div class="yellowgreenDiv">
                <richtext class ="event4"
                       onstart="start">
                </richtext>
            </div>
            <text  class="sub-title">
                richtext特有事件2
            </text>
            <div class="yellowDiv">
                <richtext class ="event5"
                       oncomplete="complete"
                          onaccessibility="onAccessibility">
                </richtext>
            </div>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="function-container">
            <text class="title">
                通用方法
            </text>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
        <div class="gradient-container">
            <text class="title">
                渐变样式
            </text>

        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="access-container">
            <text class="title">
                无障碍
            </text>

        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <div class="atom-container">
            <div class="multiMode-container">
                <text class="title">
                    原子布局
                </text>
            </div>

            <div style="background-color: #000000; width: 100%; height: 2px;">
            </div>
            <div class="multiMode-container">
                <text class="title">
                    多模输入
                </text>
            </div>
        </div>
    </div>
</div>
