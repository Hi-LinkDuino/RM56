<div class="container">
    <div class="prop-container">
        <text class="title">
            menu通用属性
        </text>

        <text class="sub-title">
            通用属性 -- id
        </text>
        <menu id="idProp">
        </menu>

        <text class="sub-title">
            通用属性 -- class
        </text>
        <menu id="classProp"
             class="classProp">
        </menu>
        <menu id="classPropNone"
             class="">
        </menu>

        <text class="sub-title">
            通用属性 -- style
        </text>
        <menu id="styleProp"
             style="width:80%;height:20px;background-color:red">
        </menu>

        <text class="sub-title">
            通用属性 -- ref
        </text>
        <menu id="refProp"
             ref="refProp">
        </menu>
        <menu id="refPropNone"
             ref="">
        </menu>

        <text class="sub-title">
            通用属性 -- disabled
        </text>
        <menu id="disabledPropTrue"
             disabled="true">
        </menu>
        <menu id="disabledPropFalse"
             disabled="false">
        </menu>
        <menu id="disabledPropNone"
             disabled="">
        </menu>

        <text class="sub-title">
            通用属性 -- focusable
        </text>
        <menu id="focusablePropTrue"
             focusable="true">
        </menu>
        <menu id="focusablePropFalse"
             focusable="false">
        </menu>
        <menu id="focusablePropNone"
             focusable="">
        </menu>

        <text class="sub-title">
            通用属性 -- data-*
        </text>
        <menu id="dataProp"
             data-menu="通用属性 -- data-menu">
        </menu>
        <menu id="dataPropNone"
             data-menu="">
        </menu>

        <text class="sub-title">
            通用属性 -- data
        </text>
        <menu class="propNewAdd"
               data="通用属性 -- data">
        </menu>
        <menu class="propNewAdd"
               data="">
        </menu>

        <text class="sub-title">
            通用属性 -- click-effect
        </text>
        <menu id="clickEffectPropSmall"
             click-effect="spring-small">
        </menu>
        <menu id="clickEffectPropMedium"
             click-effect="spring-medium">
        </menu>
        <menu id="clickEffectPropLarge"
             click-effect="spring-large">
        </menu>
        <menu id="clickEffectPropNone"
             click-effect="">
        </menu>

        <text class="sub-title">
            通用属性 -- dir
        </text>
        <menu id="dirPropRtl"
             dir="rtl">
        </menu>
        <menu id="dirPropAuto"
             dir="auto">
        </menu>
        <menu id="dirPropLtr"
             dir="ltr">
        </menu>
        <menu id="dirPropNone"
             dir="">
        </menu>

        <text class="sub-title">
            渲染属性 -- for
        </text>
        <menu id="forPropNull"
             for="">
        </menu>
        <menu id="forPropOne"
             for="{{listOne}}">
        </menu>
        <menu id="forPropThree"
             for="{{listThree}}">
        </menu>

        <text class="sub-title">
            渲染属性 -- if
        </text>
        <menu id="ifPropTrue"
             if="true">
        </menu>

        <text class="sub-title">
            渲染属性 -- show
        </text>
        <menu id="showPropTrue"
             show="true">
        </menu>
        <menu id="showPropFalse"
             show="false">
        </menu>
        <menu id="showPropNone"
             show="">
        </menu>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>
    <div class="specific-container">
        <text class="title">
            menu特有属性
        </text>

        <text id="testTarget" onclick="onTextClick" class="sub-title">
            特有属性 -- target
        </text>
        <menu id="menuTarget"
              target="testTarget">
            <option value="Item 1">Item 1</option>
            <option value="Item 2">Item 2</option>
        </menu>
        <menu id="menuTargetNone"
               target="">
        </menu>

        <text class="sub-title">
            特有属性 -- type
        </text>
        <menu id="typeClick"
               type="click">
        </menu>
        <menu id="typeLongPress"
               type="longpress">
        </menu>
        <menu id="typeNone"
              type="">
        </menu>

        <text class="sub-title">
            特有属性 -- title
        </text>
        <menu id="menuTitle"
               title="menu" >
        </menu>
        <menu id="menuTitleNone"
                title="">
        </menu>

        <text class="sub-title">
            特有属性 -- tid
        </text>
        <menu class="propNewAdd"
               tid="id">
        </menu>
        <menu class="propNewAdd"
               tid="">
        </menu>

        <text class="sub-title">
            特有属性 -- elif
        </text>
        <menu class="propNewAdd"
              if="false">
        </menu>
        <menu class="propNewAdd"
               elif="true">
        </menu>
        <menu class="propNewAdd"
               elif="false">
        </menu>

    </div>
</div>
