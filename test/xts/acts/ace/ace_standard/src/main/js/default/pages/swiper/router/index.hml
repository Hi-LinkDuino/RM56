<div class="container">
    <div class="sub-container">
    <!-- 通用样式 -->
        <div class="style-container">
            <text class="title">
                通用样式
            </text>
            <text class="sub-title">
                swiper通用样式1
            </text>
            <swiper class="swiper style1" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用样式2

            </text>
            <swiper class="swiper style2" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用样式3
            </text>
            <swiper class="swiper style3" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用样式4
            </text>
            <swiper class="swiper style4" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用样式5
            </text>
            <div class="contain1">
                <swiper class="swiper style5" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                        scrolleffect="spring">
                </swiper>
                <swiper class="swiper style6" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                        scrolleffect="spring">
                </swiper>
            </div>
            <text  class="sub-title">
                swiper特有样式
            </text>
            <swiper class="swiper style7"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>
        </div>
        <divider style="color: #000000; stroke-width: 2px;" vertical="false">
        </divider>
    <!-- 动画 -->
        <div class="ani-container">
            <text class="title">
                动画样式
            </text>
            <text class="sub-title">
                swiper动画样式1
            </text>
            <swiper class="swiper ani1" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text class="sub-title">
                swiper动画样式2
            </text>
            <swiper class="swiper ani2" id="swiper" index="0" indicator="true" loop="true" digital="false" cachedsize="-1"
                    scrolleffect="spring">
            </swiper>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 通用属性 -->
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                swiper通用属性1
            </text>
            <swiper disabled = "true"
                    focusable ="true"
                    data-name ="prop1"
                    dir ="rtl"
                    class="swiperProp"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用属性2
            </text>
            <swiper ref ="prop2"
                    disabled = "false"
                    focusable ="false"
                    data-name ="prop2"
                    dir ="ltr"
                    class="swiperProp"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用属性3
            </text>
            <swiper style="color: #679855; stroke-width: 15px; line-cap: round; margin: 5px;"
                    class="swiperProp"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper特有属性4
            </text>
            <swiper class="swiperProp"
                    autoplay="true"
                    interval="200"
                    id="swiper"
                    index="0"
                    indicator="true"
                    indicatordisabled="true"
                    loop="true"
                    duration="500"
                    digital="false"
                    vertical="true"
                    cachedsize="-1"
                    displaymode="stretch"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper特有属性5
            </text>
            <swiper class="swiperProp"
                    autoplay="false"
                    interval="1000"
                    id="swiper"
                    index="0"
                    indicator="false"
                    indicatormask="true"
                    indicatordisabled="false"
                    loop="true"
                    digital="true"
                    cachedsize="-1"
                    scrolleffect="fade">
            </swiper>

            <text  class="sub-title">
                swiper特有属性6
            </text>
            <swiper class="swiperProp"
                    autoplay="true"
                    interval="1000"
                    duration="700"
                    id="swiper"
                    index="0"
                    vertical="false"
                    indicator="true"
                    indicatormask="false"
                    loop="false"
                    digital="false"
                    displaymode="autoLinear"
                    cachedsize="-1"
                    scrolleffect="none">
            </swiper>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用事件 -->
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>
            <text  class="sub-title">
                swiper通用事件1
            </text>
            <swiper class="swiper style7 event1"
                    ontouchstart="touchStart"
                    ontouchmove="touchMove"
                    ontouchend="touchEnd"
                    ontouchcancel="touchCancel"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用事件2
            </text>
            <swiper class="swiper style7 event2"
                    ondoubleclick="doubleClick"
                    onlongpress="longPress"
                    onfocus="focus"
                    onblur="blur"
                    onkey="key"
                    onswipe="swipe"
                    onattached="attached"
                    ondetached="detached"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiper通用事件3
            </text>
            <swiper class="swiper style7"
                    ontouchstart="functionTest1"
                    onpinchstart="pinchStart"
                    onpinchupdate="pinchUpdate"
                    onpinchend="pinchEnd"
                    onpinchcancel="pinchCancel"
                    ondragstart="dragStart"
                    ondrag="drag"
                    ondragend="dragEnd"
                    ondragenter="dragEnter"
                    ondragover="dragOver"
                    ondragleave="dragLeave"
                    ondrop="drop"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>

            <text  class="sub-title">
                swiperOnAccessibility
            </text>
            <swiper onaccessibility="onAccessibility"></swiper>

            <text  class="sub-title">
                swiper特有事件1
            </text>
            <swiper class="swiper style7"
                    change = "changeTest"
                    rotation = "rotationTest"
                    animationfinish = "animationfinish"
                    id="swiper"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用方法 -->
        <div class="function-container">
            <text class="title">
                通用方法
            </text>
            <text class="sub-title">
                swiper通用方法1
            </text>
            <swiper class="swiper style7"
                    id="swiper1"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring"
                    ontouchstart="functionTest1">
            </swiper>

            <text class="sub-title">
                swiper通用方法2
            </text>
            <swiper class="swiper style7"
                    ontouchstart="functionTest2"
                    id="swiper2"
                    index="0"
                    indicator="true"
                    loop="true"
                    digital="false"
                    cachedsize="-1"
                    scrolleffect="spring">
            </swiper>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
