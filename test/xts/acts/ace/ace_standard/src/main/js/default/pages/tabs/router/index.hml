        <tabs class = "tabs style1"
              index="0"
              vertical="false"
              onchange="change">
        </tabs>

        <text  class="sub-title">
            tabs通用样式2
        </text>
        <tabs class = "tabs style2"
              index="0"
              vertical="false"
              onchange="change">
        </tabs>

        <text  class="sub-title">
            tabs通用样式3
        </text>
        <tabs class = "tabs style3"
              index="0"
              vertical="false"
              onchange="change">
        </tabs>

