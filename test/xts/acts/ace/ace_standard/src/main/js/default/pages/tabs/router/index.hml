    <div class="sub-container">
        <!-- 通用属性 -->
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                tabs通用属性1
            </text>
            <tabs id="prop1"
                  disabled = "true"
                  focusable ="true"
                  data-name ="prop1"
                  click-effect="spring-medium"
                  dir ="rtl"
                  class = "tabs style7"
                  index="0"
                  vertical="false"
                  onchange="change">
            </tabs>

            <text  class="sub-title">
                tabs通用属性2
            </text>
            <tabs disabled = "false"
                  focusable ="false"
                  data-name ="prop2"
                  click-effect="spring-large"
                  dir ="ltr"
                  class = "tabs style7 prop2"
                  ref="prop2"
                  index="0"
                  vertical="false"
                  onchange="change">
            </tabs>

            <text  class="sub-title">
                tabs通用属性3
            </text>
            <tabs style="background-color: seagreen;  margin: 5px;"
                  class = "tabs style7"
                  index="0"
                  vertical="false"
                  onchange="change">
            </tabs>

            <text  class="sub-title">
                tabs特有属性4
            </text>
            <tabs class = "tabs style7 prop4"
                  index="1"
                  vertical="false"
                  onchange="change">
            </tabs>

            <text  class="sub-title">
                tabs特有属性5
            </text>
            <tabs class = "tabs style7 prop5"
                  index="0"
                  vertical="true"
                  onchange="change">
            </tabs>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <!-- 通用事件 -->
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>
            <text  class="sub-title">
                tabs通用事件1
            </text>
            <tabs class = "tabs tstyle event1"
                  ontouchstart="touchStart"
                  ontouchmove="touchMove"
                  ontouchend="touchEnd"
                  ontouchcancel="touchCancel"
                  index="0" vertical="false"
                  onchange="change">
            </tabs>

            <text  class="sub-title">
                tabs通用事件2
            </text>
            <tabs class = "tabs tstyle event2"
                  onclick="click"
                  ondoubleclick="doubleClick"
                  onlongpress="longPress"
                  onfocus="focus"
                  onblur="blur"
                  onkey="key"
                  onswipe="swipe"
                  onattached="attached"
                  ondetached="detached"
                  index="0" vertical="false"
                  onchange="change">
            </tabs>
            <text  class="sub-title">
                tabs通用事件3
            </text>
            <tabs class = "tabs tstyle event3"
                  ontouchstart="functionTest1"
                  onpinchstart="pinchStart"
                  onpinchupdate="pinchUpdate"
                  onpinchend="pinchEnd"
                  onpinchcancel="pinchCancel"
                  ondragstart="dragStart"
                  ondrag="drag"
                  ondragend="dragEnd"
                  ondragenter="dragEnter"
                  ondragover="dragOver"
                  ondragleave="dragLeave"
                  ondrop="drop">
            </tabs>

            <text  class="sub-title">
                tabs特有事件1
            </text>
            <tabs class = "tabs tstyle event4"
                  index="0" vertical="false"
                  onchange="change">
            </tabs>

        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
