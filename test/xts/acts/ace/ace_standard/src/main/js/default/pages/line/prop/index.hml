<div class="container">
    <div class="prop-container">
        <text class="title">
            line通用属性
        </text>
        <text class="sub-title">
            通用属性 -- opacity
        </text>
        <div style="height: 20px;width: 150px;">
            <svg width="50px" height="20px">
                <line id ="opacityPropOne" x1="10" x2="50" y1="10" y2="10" stroke-width="4" stroke="red" opacity="0.5"></line>
            </svg>
            <svg width="50px" height="20px">
                <line id ="opacityPropTwo" x1="10" x2="50" y1="10" y2="10" stroke-width="4" stroke="red" opacity="1"></line>
            </svg>
            <svg width="50px" height="20px">
                <line id ="opacityPropThree" x1="10" x2="50" y1="10" y2="10" stroke-width="4" stroke="red" opacity="1.5"></line>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke
        </text>
        <svg width="100px" height="20px">
            <line id ="strokeProp" x1="10" x2="50" y1="10" y2="10" stroke-width="4" stroke="red"></line>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-dasharray
        </text>
        <svg width="100px" height="20px">
            <line id ="strokeDashArrayProp" x1="10" x2="50" y1="10" y2="10" stroke-width="4" stroke = "red" stroke-dasharray="2 3"></line>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-dashoffset
        </text>
        <svg width="100px" height="20px">
            <line id ="strokeDashOffsetProp" x1="10" x2="50" y1="10" y2="10" stroke-width="4" stroke = "red" stroke-dashoffset="23"></line>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-linejoin
        </text>
        <div style="height: 20px;width: 150px;">
            <svg  width="50px" height="20px">
                <line id ="strokeLineJoinPropOne" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-linejoin="bevel"></line>
            </svg>
            <svg  width="50px" height="20px">
                <line id ="strokeLineJoinPropTwo" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-linejoin="miter"></line>
            </svg>
            <svg  width="50px" height="20px">
                <line id ="strokeLineJoinPropThree" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-linejoin="round"></line>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke-linecap
        </text>
        <div style="height: 20px;width: 150px;">
            <svg width="50px" height="20px" >
                <line id ="strokeLineCapPropOne" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-linecap="butt"></line>
            </svg>
            <svg width="50px" height="20px" >
                <line id ="strokeLineCapPropTwo" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-linecap="round"></line>
            </svg>
            <svg width="50px" height="20px" >
                <line id ="strokeLineCapPropThree" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-linecap="square"></line>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke-miterlimit
        </text>
        <svg width="100px" height="20px" >
            <line id ="strokeMiterLimitProp" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                  stroke = "red" stroke-miterlimit="10"></line>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-opacity
        </text>
        <div style="height: 20px;width: 150px;">
            <svg width="50px" height="20px">
                <line id ="strokeOpacityPropOne" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-opacity="0.5"></line>
            </svg>
            <svg width="50px" height="20px">
                <line id ="strokeOpacityPropTwo" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-opacity="1"></line>
            </svg>
            <svg width="50px" height="20px">
                <line id ="strokeOpacityPropThree" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                      stroke = "red" stroke-opacity="1.5"></line>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke-width
        </text>
        <svg width="100px" height="20px" >
            <line id ="strokeWidthProp" x1="10" x2="50" y1="10" y2="10" stroke-width="4"
                  stroke = "red" stroke-width="6"></line>
        </svg>

        <text class="sub-title">
            通用属性 -- transform
        </text>
        <svg width="100px" height="100px">
            <line id ="transformProp" x1="10" x2="50" y1="50" y2="50" stroke-width="4" stroke="red"
                  transform="translate(10,5) scale(0.5 1.3) rotate(20 40 90) skewX(10) skewY(-15)"></line>
        </svg>
    </div>

    <divider style="color: #000000; stroke-width: 2px;" vertical="true">
    </divider>

    <div class="prop-container">
        <text class="title">
            line特有属性
        </text>

        <text class="sub-title">
            特有属性-id
        </text>
        <svg width="100px" height="40px">
            <line id ="idProp" x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red"></line>
        </svg>

        <text class="sub-title">
            特有属性-x1
        </text>
        <svg  width="100px" height="40px">
            <svg width="100px" height="40px" fill="red" >
                <line id ="x1Prop" x1="10"  stroke-width="4" stroke="red"></line>
            </svg>
        </svg>

        <text class="sub-title">
            特有属性-x2
        </text>
        <svg  width="100px" height="40px">
            <svg width="100px" height="40px" fill="red" >
                <line id ="x2Prop" x2="100" stroke-width="4" stroke="red"></line>
            </svg>
        </svg>

        <text class="sub-title">
            特有属性-y1
        </text>
        <svg  width="100px" height="40px">
            <svg width="100px" height="40px" fill="red" >
                <line id ="y1Prop" y1="10" stroke-width="4" stroke="red"></line>
            </svg>
        </svg>

        <text class="sub-title">
            特有属性-y2
        </text>
        <svg  width="100px" height="40px">
            <svg width="100px" height="40px" fill="red" >
                <line id ="y1Prop" y2="10" stroke-width="4" stroke="red"></line>
            </svg>
        </svg>

        <text class="sub-title">
            特有属性-if
        </text>
        <svg width="100px" height="50px">
            <line id ="ifPropTrue" x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" if="true"></line>
        </svg>

        <text class="sub-title">
            特有属性-show
        </text>
        <svg width="200px" height="50px">
            <line id ="showPropTrue"  x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" show="true"></line>
            <line id ="showPropFalse"  x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" show="false"></line>
            <line id ="showPropNone"  x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" show=""></line>
        </svg>

        <text class="sub-title">
            特有属性-elif
        </text>
        <svg width="200px" height="50px">
            <line x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" if="false"></line>
            <line id="elifPropTrue" x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" elif="true"></line>
            <line id="elifPropFalse" x1="10" x2="100" y1="10" y2="10" stroke-width="4" stroke="red" elif="false"></line>
        </svg>

    </div>
</div>
