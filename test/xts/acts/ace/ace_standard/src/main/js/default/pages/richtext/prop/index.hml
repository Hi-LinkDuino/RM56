<div class="container">
    <div class="prop-container">
        <text class="title">
            richtext通用属性
        </text>

        <text class="sub-title">
            通用属性 -- id
        </text>
        <div class="idPropDiv">
            <richtext id="idProp">{{ content }}
            </richtext>
        </div>

        <text class="sub-title">
            通用属性 -- class
        </text>
        <div class="classPropDiv">
            <richtext id="classProp"
                 class="classProp">
                {{ content }}
            </richtext>
        </div>
        <div class="classPropNoneDiv">
            <richtext id="classPropNone"
                 class="">
                {{ content }}
            </richtext>
        </div>


        <text class="sub-title">
            通用属性 -- style
        </text>
        <div class="stylePropDiv">
            <richtext id="styleProp"
                 style="visibility: visible;">
                {{ content }}
            </richtext>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>
    <div class="specific-container">
        <text class="title">
            richtext特有属性
        </text>
    </div>
</div>
