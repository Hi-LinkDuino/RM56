<div class="container">
    <div class="prop-container">
        <text class="title">
            polygon通用属性
        </text>

        <text class="sub-title">
            通用属性 -- fill
        </text>
        <svg width="100" height="35" >
            <polygon id ="fillProp" points="10,30 20,0 30,30 40,0" fill="red"></polygon >
        </svg>

        <text class="sub-title">
            通用属性 -- fill-opacity
        </text>
        <div style="height: 35px;width: 150px;">
            <svg width="50px" height="35px">
                <polygon id ="fillOpacityPropOne" points="10,30 20,0 30,30 40,0" fill-opacity="0.5" fill="red"></polygon>
            </svg>
            <svg width="50px" height="35px">
                <polygon id ="fillOpacityPropTwo" points="10,30 20,0 30,30 40,0" fill-opacity="1" fill="red"></polygon>
            </svg>
            <svg width="50px" height="35px">
                <polygon id ="fillOpacityPropThree" points="10,30 20,0 30,30 40,0" fill-opacity="1.5" fill="red"></polygon>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- fill-rule
        </text>
        <div style="height: 35px;width: 150px;">
            <svg width="50px" height="35px">
                <polygon id ="fillRulePropOne" points="10,30 20,0 30,30 40,0" fill-rule="evenodd" fill="red"></polygon>
            </svg>
            <svg  width="50px" height="35px">
                <polygon id ="fillRulePropTwo" points="10,30 20,0 30,30 40,0" fill-rule="nonzero" fill="red"></polygon>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- opacity
        </text>
        <div style="height: 35px;width: 150px;">
            <svg width="50px" height="35px">
                <polygon id ="opacityPropOne" points="10,30 20,0 30,30 40,0" fill="red" opacity="0.5"></polygon>
            </svg>
            <svg width="50px" height="35px">
                <polygon id ="opacityPropTwo" points="10,30 20,0 30,30 40,0" fill="red" opacity="1"></polygon>
            </svg>
            <svg width="50px" height="35px">
                <polygon id ="opacityPropThree" points="10,30 20,0 30,30 40,0" fill="red" opacity="1.5"></polygon>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke
        </text>
        <svg width="100px" height="35px">
            <polygon id ="strokeProp" points="10,30 20,0 30,30 40,0" fill="white" stroke = "red"></polygon>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-dasharray
        </text>
        <svg width="100px" height="35px">
            <polygon id ="strokeDashArrayProp" points="10,30 20,0 30,30 40,0" fill="white" stroke = "red" stroke-dasharray="2 3"></polygon>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-dashoffset
        </text>
        <svg width="100px" height="35px">
            <polygon id ="strokeDashOffsetProp" points="10,30 20,0 30,30 40,0" fill="white" stroke = "red" stroke-dashoffset="23"></polygon>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-linejoin
        </text>
        <div style="height: 35px;width: 150px;">
            <svg  width="50px" height="35px">
                <polygon id ="strokeLineJoinPropOne" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-linejoin="bevel"></polygon>
            </svg>
            <svg  width="50px" height="35px">
                <polygon id ="strokeLineJoinPropTwo" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-linejoin="miter"></polygon>
            </svg>
            <svg  width="50px" height="35px">
                <polygon id ="strokeLineJoinPropThree" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-linejoin="round"></polygon>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke-linecap
        </text>
        <div style="height: 35px;width: 150px;">
            <svg width="50px" height="35px" >
                <polygon id ="strokeLineCapPropOne" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-linecap="butt"></polygon>
            </svg>
            <svg width="50px" height="35px" >
                <polygon id ="strokeLineCapPropTwo" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-linecap="round"></polygon>
            </svg>
            <svg width="50px" height="35px" >
                <polygon id ="strokeLineCapPropThree" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-linecap="square"></polygon>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke-miterlimit
        </text>
        <svg width="100px" height="35px" >
            <polygon id ="strokeMiterLimitProp" points="10,30 20,0 30,30 40,0" fill="white"
                  stroke = "red" stroke-miterlimit="10"></polygon>
        </svg>

        <text class="sub-title">
            通用属性 -- stroke-opacity
        </text>
        <div style="height: 35px;width: 150px;">
            <svg width="50px" height="35px">
                <polygon id ="strokeOpacityPropOne" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-opacity="0.5"></polygon>
            </svg>
            <svg width="50px" height="35px">
                <polygon id ="strokeOpacityPropTwo" points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-opacity="1"></polygon>
            </svg>
            <svg width="50px" height="35px">
                <polygon id ="strokeOpacityPropThree"points="10,30 20,0 30,30 40,0" fill="white"
                      stroke = "red" stroke-opacity="1.5"></polygon>
            </svg>
        </div>

        <text class="sub-title">
            通用属性 -- stroke-width
        </text>
        <svg width="100px" height="35px" >
            <polygon id ="strokeWidthProp" points="10,30 20,0 30,30 40,0" fill="white"
                  stroke = "red" stroke-width="4"></polygon>
        </svg>

        <text class="sub-title">
            通用属性 -- transform
        </text>
        <svg width="100px" height="40px">
            <polygon id ="transformProp" points="10,30 20,0 30,30 40,0" fill="red"
                  transform="translate(10,5) scale(0.5 1.3) rotate(20 40 90) skewX(10) skewY(-15)"></polygon>
        </svg>
    </div>

    <divider style="color: #000000; stroke-width: 2px;" vertical="true">
    </divider>

    <div class="prop-container">
        <text class="title">
            polygon特有属性
        </text>

        <text class="sub-title">
            特有属性-id
        </text>
        <svg width="100px" height="40px">
            <polygon id ="idProp" points="10,30 20,0 30,30 40,0"  fill="red"></polygon>
        </svg>

        <text class="sub-title">
            特有属性- point
        </text>
        <svg width="100px" height="40px">
            <polygon id ="pointProp" points="10,30 20,0 30,30 40,0" fill="red" stroke="blue"></polygon>
        </svg>

        <text class="sub-title">
            特有属性-if
        </text>
        <svg width="100px" height="50px">
            <polygon id ="ifPropTrue" points="10,30 20,0 30,30 40,0" fill="red" if="true"></polygon>
        </svg>

        <text class="sub-title">
            特有属性-show
        </text>
        <svg width="200px" height="50px">
            <polygon id ="showPropTrue" points="10,30 20,0 30,30 40,0" fill="red"  show="true"></polygon>
            <polygon id ="showPropFalse" points="10,30 20,0 30,30 40,0" fill="red" show="false"></polygon>
            <polygon id ="showPropNone" points="10,30 20,0 30,30 40,0" fill="red" show=""></polygon>
        </svg>

        <text class="sub-title">
            特有属性-elif
        </text>
        <svg width="200px" height="50px" fill="red" >
            <polygon points="10,30 20,0 30,30 40,0" if="false"></polygon>
            <polygon id="elifPropTrue" points="10,30 20,0 30,30 40,0" elif="true"></polygon>
            <polygon id="elifPropFalse" points="10,30 20,0 30,30 40,0" elif="false"></polygon>
        </svg>
    </div>


</div>
