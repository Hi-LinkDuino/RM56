            <text  class="sub-title">
                line通用属性9
            </text>
            <svg width="200" height="60">
                <line id="prop9" x1="10" x2="180" y1="10" y2="10" stroke="blue" stroke-width="4" transform="skewY(10)"></line>
            </svg>

            <text  class="sub-title">
                line通用属性10
            </text>
            <svg  width="200" height="50"  >
                <line id="prop10" x1="10" x2="180" y1="10" y2="10" stroke="red" stroke-width="4"  stroke-linejoin="square"></line>
            </svg>

            <text class="sub-title">
                line通用属性11
            </text>
            <svg  width="200" height="50"  >
                <line id="prop11" x1="10" x2="180" y1="10" y2="10" stroke="red" stroke-width="4"  stroke-miterlimit="10"></line>
            </svg>

