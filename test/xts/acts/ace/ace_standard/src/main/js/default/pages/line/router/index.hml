<div class="container">
    <div class="sub-container">
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                line通用属性1
            </text>
            <svg width="200" height="50">
                <line id="prop1" x1="10" x2="180" y1="20" y2="20"
                      stroke="green" stroke-width="4" ></line>
            </svg>

            <text class="sub-title">
                line通用属性2
            </text>
            <svg width="200" height="50">
                <line id="prop2" x1="10" x2="180" y1="20" y2="20"
                      stroke="white" stroke-width="4" opacity="0.5"></line>
            </svg>

            <text  class="sub-title">
                line通用属性3
            </text>
            <svg width="200" height="50">
                <line  id="prop3" x1="10" x2="180" y1="20" y2="20" stroke="red"
                      stroke-width="4"  stroke-dasharray="5 3" stroke-dashoffset="3"></line>
            </svg>
            <text  class="sub-title">
                line通用属性4
            </text>
            <svg width="200" height="50">
                <line id="prop4" x1="10" x2="180" y1="20" y2="20" stroke="black"
                      stroke-width="4" stroke-linecap="round"></line>
            </svg>
            <text  class="sub-title">
                line通用属性5
            </text>
            <svg width="200" height="50">
                <line  id="prop5" x1="10" x2="180" y1="20" y2="20" stroke="black"
                      stroke-width="4" stroke-linecap="butt"></line>
            </svg>

            <text  class="sub-title">
                line通用属性6
            </text>
            <svg width="200" height="50">
                <line id="prop6" x1="10" x2="180" y1="20" y2="20" stroke="black"
                      stroke-width="4" stroke-linecap="square"></line>
            </svg>

            <text  class="sub-title">
                line通用属性7
            </text>
            <svg width="200" height="50">
                <line  id="prop7" x1="10" x2="180" y1="20" y2="20" stroke="blue" stroke-width="4" transform="rotate(-5)"></line>
            </svg>

            <text  class="sub-title">
                line通用属性8
            </text>
            <svg width="200" height="50">
                <line id="prop8" x1="10" x2="180" y1="20" y2="20" stroke="yellow" stroke-width="4" transform="skewX(20)"></line>
