<div class="container">
    <div class="sub-container">
        <div class="style-container">
            <text class="title">
                通用样式
            </text>

            <text class="sub-title">
                image通用样式1
            </text>
            <image id="style1" class="style1" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用样式2
            </text>
            <image id="style2" class="style2" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用样式3
            </text>
            <image id="style3" class="style3" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用样式4
            </text>
            <image id="style4" class="style4" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用样式5
            </text>
            <div class="contain1">
                <image id="style5" class="style5" src="common/images/image.png">
                </image>
                <image id="style6" class="style6" src="common/images/image.png">
                </image>
            </div>

            <text  class="sub-title">
                image特有样式
            </text>
            <image id="style7" class="style7" src="common/images/image.png">
            </image>
            <image id="style8" class="style8" src="common/images/image.png">
            </image>
        </div>

        <divider style="color: #000000; stroke-width: 2px;" vertical="false">
        </divider>
    <!-- 动画 -->
        <div class="ani-container">
            <text class="title">
                动画样式
            </text>

            <text class="sub-title">
                image动画样式1
            </text>
            <image id="ani1" class="ani1" src="common/images/image.png">
            </image>

            <text class="sub-title">
                image动画样式2
            </text>
            <image id="ani2" class="ani2" src="common/images/image.png">
            </image>

        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 通用属性 -->
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                image通用属性1
            </text>
            <image id="prop1"
                   class="prop1"
                   disabled = "true"
                   focusable ="true"
                   data-name ="prop1"
                   click-effect="spring-medium"
                   dir ="rtl" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用属性2
            </text>
            <image id="prop2"
                   class="prop2"
                   ref ="prop2"
                   disabled = "false"
                   focusable ="false"
                   data-name ="prop2"
                   click-effect="spring-large"
                   dir ="ltr"
                   src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用属性3
            </text>
            <image id="prop3" style="color: #679855; margin: 5px;width: 100%;height: 40px;" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image特有属性4
            </text>
            <image id="prop4" class="prop4" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image特有属性5
            </text>
            <image id="prop5" class="prop5" src="common/images/image.png">
            </image>

        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用事件 -->
        <div class="event-container" >
            <text class="title">
                通用事件
            </text>
            <text  class="sub-title">
                image通用事件1
            </text>
            <image id="event1" class ="event1" ontouchstart="touchStart" ontouchmove="touchMove"
                   ontouchend="touchEnd" ontouchcancel="touchCancel" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用事件2
            </text>
            <image id="event2" class="event2" onclick="click" ondoubleclick="doubleClick"
                   onlongpress="longPress" onfocus="focus" onblur="blur" onkey="key"
                   onswipe="swipe" onattached="attached" ondetached="detached" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image通用事件3
            </text>
            <image id="event3" class="event3" onpinchstart="pinchStart" onpinchupdate="pinchUpdate"
                   onpinchend="pinchEnd" onpinchcancel="pinchCancel"
                   ondragstart="dragStart" ondrag="drag"
                   ondragend="dragEnd" ondragenter="dragEnter"
                   ondragover="dragOver" ondragleave="dragLeave"
                   ondrop="drop" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image特有事件1
            </text>
            <image id="event4" class="event4"  src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image特有事件2
            </text>
            <image id="event5" class="event5" src="common/images/image.png">
            </image>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 通用方法 -->
        <div class="function-container">
            <text class="title">
                通用方法
            </text>
            <text class="sub-title">
                image通用方法1
            </text>
            <image id="function1" class="function1" ontouchstart="functionTest1" src="common/images/image.png">
            </image>

            <text class="sub-title">
                image通用方法2
            </text>
            <image id="function2" class="function2" ontouchstart="functionTest2" src="common/images/image.png">
            </image>

            <text class="sub-title">
                image通用方法3
            </text>
            <image id="function3" class="function3" ontouchstart="functionTest3" src="common/images/image.png">
            </image>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
    <!-- 渐变样式 -->
        <div class="gradient-container">
            <text class="title">
                渐变样式
            </text>

            <text class="sub-title">
                image渐变样式1
            </text>
            <image id="gradient1" class="gradient1" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image渐变样式2
            </text>
            <image id="gradient2" class="gradient2" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image渐变样式3
            </text>
            <image id="gradient3" class="gradient3" src="common/images/image.png">
            </image>

            <text  class="sub-title">
                image渐变样式4
            </text>
            <image id="gradient4" class="gradient4" src="common/images/image.png">
            </image>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 无障碍 -->
        <div class="access-container">
            <text class="title">
                无障碍
            </text>
            <text class="sub-title">
                image无障碍1
            </text>
            <image id="access1" class="access1"
                   onaccessibility="onAccessibility"
                   accessibilitygroup ="true"
                   accessibilitytext="这是div"
                   accessibilitydescription="点击此按键会弹出一个对话框"
                   accessibilityimportance="no-hide-descendants"
                   src="common/images/image.png">
            </image>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    <!-- 原子布局和多模输入 -->
        <div class="atom-container">
            <text class="title">
                原子布局
            </text>
            <text class="sub-title">
                image原子布局1
            </text>
            <div style="flex-direction: row;height: 30px;width: 100%;">
                <image id="atomA1" src="common/images/image.png">
                </image>
                <image id="atomA2" src="common/images/image.png">
                </image>
                <image id="atomA3" src="common/images/image.png">
                </image>
            </div>
            <text class="sub-title">
                image原子布局2
            </text>
            <div style="flex-direction: row;height: 30px;width: 100%;">
                <image id="atomB1" src="common/images/image.png">
                </image>
                <image id="atomB2" src="common/images/image.png">
                </image>
                <image id="atomB3" src="common/images/image.png">
                </image>
            </div>
            <text class="sub-title">
                image原子布局3
            </text>
            <div style="flex-direction: row;height:40px;width: 100%;">
                <image id="atomC1" src="common/images/image.png">
                </image>
                <image id="atomC2" src="common/images/image.png">
                </image>
                <image id="atomC3" src="common/images/image.png">
                </image>
            </div>
            <div style="background-color: #000000; width: 100%; height: 2px;">
            </div>
            <div class="multimode-container">
                <text class="title">
                    多模输入
                </text>
                <text class="sub-title">
                    image多模输入1
                </text>
                <image id="multiMode1"
                       class="multimode1"
                       voicelabel = "voice"
                       subscriptflag="on"
                       subscriptlabel="div"
                       scenelabel="common"
                       src="common/images/image.png">
                </image>
            </div>
        </div>
    </div>
</div>






