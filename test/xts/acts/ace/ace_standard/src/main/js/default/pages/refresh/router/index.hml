                     dir ="ltr"
                     offset="10px"
                     refreshing="{{fresh}}"
                     onrefresh="refresh">
            </refresh>

            <text  class="sub-title">
                refresh通用属性3
            </text>
            <refresh class="style7"
                     style="color: #679855; stroke-width: 15px; line-cap: round; margin: 5px;"
