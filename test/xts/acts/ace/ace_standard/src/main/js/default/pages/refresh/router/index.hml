        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                refresh通用属性1
            </text>
            <refresh class="style7"
                     id="prop1"
                     disabled = "true" focusable ="true"
                     data-name ="prop1" click-effect="spring-medium"
                     dir ="rtl"
                     offset="10px"
                     refreshing="{{fresh}}"
                     onrefresh="refresh">
            </refresh>

            <text  class="sub-title">
                refresh通用属性2
            </text>
            <refresh class="style7 prop2"
                     ref ="prop2"
