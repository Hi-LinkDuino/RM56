                     onrefresh="refresh">
            </refresh>

            <text  class="sub-title">
                refresh特有属性4
            </text>
            <refresh class="style7"
                     offset="5px"
                     type="auto"
                     lasttime="true"
                     friction="42"
                     timeoffset="2px"
                     refreshing="true"
                     onrefresh="refresh">
