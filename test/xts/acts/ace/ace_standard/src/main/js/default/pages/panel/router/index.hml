<div class="container">
    <div class="sub-container">
        <!-- 通用样式 -->
        <div class="style-container">
            <text class="title">
                样式
            </text>
            <text class="sub-title">
                panel样式1
            </text>
            <button  class="btn-div" type="capsule"
                    value="Click here"
                    onclick="showPanel1">
            </button>
            <panel id="simplePanel1"
                   class="style1" type="foldable">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanel1">
                    </button>
                </div>
            </panel>

            <text class="sub-title">
                panel样式2
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanel2">
            </button>
            <panel id="simplePanel2" type="foldable"
                   class="style2">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanel2">
                    </button>
                </div>
            </panel>
            <text class="sub-title">
                panel样式3
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanel3">
             </button>
            <panel id="simplePanel3" type="foldable"
                   class="style3">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanel3">
                    </button>
                </div>
            </panel>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
        <!-- 通用属性 -->
        <div class="prop-container">
            <text class="title">
                通用属性
            </text>
            <text class="sub-title">
                panel通用属性1
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelPro1">
            </button>
            <panel id = "prop1"
                   class="prop1"
                   type="foldable"
                   disabled = "true"
                   focusable ="true"
                   data-name ="prop1"
                   click-effect="spring-medium"
                   dir ="rtl">
                <div class="btn-div">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelPro1">
                    </button>
                </div>
            </panel>
            <text  class="sub-title">
                panel通用属性2
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelPro2">
            </button>
            <panel class="prop2" ref ="prop2" type="foldable"
                   disabled = "false" focusable ="false"
                   data-name ="prop2" click-effect="spring-large"
                   dir ="ltr"
                   id = "prop2">
                <div class="btn-div">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelPro1">
                    </button>
                </div>
            </panel>
            <text  class="sub-title">
                panel通用属性3
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelPro3">
            </button>
            <panel id="simplePanelPro3" style="color: #679855; margin: 5px;" class="prop3" type="foldable">
                <div class="btn-div">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelPro1">
                    </button>
                </div>
            </panel>

            <text  class="sub-title">
                panel特有属性4
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelPro4">
            </button>
            <panel id="simplePanelPro4"
                   class="style1"
                   type="foldable"
                   dragbar="true"
                   mode="mini"
                   onsizechange="changeMode"
                   miniheight="200px">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelPro4">
                    </button>
                </div>
            </panel>
            <text  class="sub-title">
                panel特有属性5
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelPro5">
            </button>
            <panel id="simplePanelPro5"
                   class="style2"
                   type="minibar"
                   dragbar="false"
                   mode="half"
                   halfheight="400px"
                   onsizechange="changeMode"
                   miniheight="200px">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelPro5">
                    </button>
                </div>
            </panel>

            <text  class="sub-title">
                panel特有属性6
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelPro6">
            </button>
            <panel id="simplePanelPro6"
                   class="style3"
                   type="temporary"
                   mode="full"
                   fullheight="800px"
                   onsizechange="changeMode"
                   miniheight="200px">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelPro6">
                    </button>
                </div>
            </panel>
        </div>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="sub-container">
        <!-- 渐变样式 -->
        <div class="gradient-container">
            <text class="title">
                渐变样式
            </text>
            <text class="sub-title">
                panel渐变样式1
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelA">
            </button>
            <panel id="simplePanelA1"
                   type="foldable"
                   class="gradient1">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelA">
                    </button>
                </div>
            </panel>
            <text  class="sub-title">
                panel渐变样式2
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelB">
            </button>
            <panel id="simplePanelB1"
                   type="foldable"
                   class="gradient2">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelB">
                    </button>
                </div>
            </panel>
            <text  class="sub-title">
                panel渐变样式3
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelC">
            </button>
            <panel id="simplePanelC1"
                   type="foldable"
                   class="gradient3">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelC">
                    </button>
                </div>
            </panel>
            <text  class="sub-title">
                panel渐变样式4
            </text>
            <button  class="btn-div" type="capsule"
                     value="Click here"
                     onclick="showPanelD">
            </button>
            <panel id="simplePanelD1"
                   type="foldable"
                   class="gradient4">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelD">
                    </button>
                </div>
            </panel>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
        <!-- 无障碍 -->
        <div class="access-container">
            <text class="title">
                无障碍
            </text>
            <button  class="btn-div" type="capsule" style="height: 30px;"
                     value="Click here"
                     onclick="showPanelNo">
            </button>
            <panel id="simplePanelNo"
                    type="foldable"
                   class="access1" accessibilitygroup ="true"
                   accessibilitytext="这是panel"
                   accessibilitydescription="点击此按键会弹出一个对话框"
                   accessibilityimportance="no-hide-descendants">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelNo">
                    </button>
                </div>
            </panel>
            <panel onaccessibility="onAccessibility"
                   type="foldable"
                   class="access1" accessibilitygroup ="true"
                   accessibilitytext="这是panel"
                   accessibilitydescription="点击此按键会弹出一个对话框"
                   accessibilityimportance="no-hide-descendants">
                <div class="inner-btn">
                    <button type="capsule"
                            value="Close"
                            onclick="closePanelNo">
                    </button>
                </div>
            </panel>
        </div>
        <div style="background-color: #000000; width: 100%; height: 2px;">
        </div>
    </div>
</div>

