           keepalive="true"
           clickable="true"
           arrowoffset="100px"
           onvisibilitychange="visibilitychange"
           onclick="hidepopupani1">
        <text class="text">popupani-1</text>
    </popup>

    <popup id="popupani2"
           class="ani2"
           target="textani2"
           placement="top"
           keepalive="false"
