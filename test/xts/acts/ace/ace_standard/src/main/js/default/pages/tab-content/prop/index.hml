<div class="container">
    <div class="prop-container">
        <text class="title">
            tab-content通用属性
        </text>

        <text class="sub-title">
            通用属性 -- id
        </text>
        <tabs class = "tabs">
            <tab-content id="idProp">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- class
        </text>
        <tabs class = "tabs">
            <tab-content id="classProp"
                  class="classProp">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="classPropNone"
                  class="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- style
        </text>
        <tabs class = "tabs">
            <tab-content id="styleProp"
                  style="width:80%;height:20px;background-color:red">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>


        <text class="sub-title">
            通用属性 -- ref
        </text>
        <tabs class = "tabs">
            <tab-content id="refProp"
                  ref="refProp">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="refPropNone"
                  ref="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- disabled
        </text>
        <tabs class = "tabs">
            <tab-content id="disabledPropTrue"
                  disabled="true">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="disabledPropFalse"
                  disabled="false">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="disabledPropNone"
                  disabled="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- focusable
        </text>
        <tabs class = "tabs">
            <tab-content id="focusablePropTrue"
                  focusable="true">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="focusablePropFalse"
                  focusable="false">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="focusablePropNone"
                  focusable="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- data-*
        </text>
        <tabs class = "tabs">
            <tab-content id="dataProp"
                  data-tabBar="通用属性 -- data-tab-content">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="dataPropNone"
                  data-tab-content="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- data
        </text>
        <tabs class = "tabs">
            <tab-content class="propNewAdd"
                     data="通用属性 -- data">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content class="propNewAdd"
                     data="">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- click-effect
        </text>
        <tabs class = "tabs">
            <tab-content id="clickEffectPropSmall"
                  click-effect="spring-small">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="clickEffectPropMedium"
                  click-effect="spring-medium">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="clickEffectPropLarge"
                  click-effect="spring-large">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="clickEffectPropNone"
                  click-effect="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            通用属性 -- dir
        </text>
        <tabs class = "tabs">
            <tab-content id="dirPropRtl"
                  dir="rtl">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="dirPropAuto"
                  dir="auto">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="dirPropLtr"
                  dir="ltr">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="dirPropNone"
                  dir="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            渲染属性 -- for
        </text>
        <tabs class = "tabs">
            <tab-content id="forPropNull"
                  for="">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="forPropOne"
                  for="{{listOne}}">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="forPropThree"
                  for="{{listThree}}">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            渲染属性 -- if
        </text>
        <tabs class = "tabs">
            <tab-content id="ifPropTrue"
                  if="true">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>


        <text class="sub-title">
            渲染属性 -- show
        </text>
        <tabs class = "tabs">
            <tab-content id="showPropTrue"
                  show="true">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="showPropFalse"
                  show="false">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
        <tabs class = "tabs">
            <tab-content id="showPropNone"
                  show="false">
                <text class="tab-text">First</text>
                <text class="tab-text">Second</text>
            </tab-content>
        </tabs>
    </div>

    <div style="background-color: #000000; width: 2px; height: 100%;">
    </div>

    <div class="specific-container">
        <text class="title">
            tab-content特有属性
        </text>
        <text class="sub-title">
            特有属性 -- scrollable
        </text>
        <tabs class = "mode">
            <tab-content id="scrollableTrue"
                         scrollable="true">
                <text class="textFont">First</text>
            </tab-content>
        </tabs>
        <tabs class = "mode">
            <tab-content id="scrollableFalse"
                         scrollable="false">
                <text class="textFont">First</text>
            </tab-content>
        </tabs>
        <tabs class = "mode">
            <tab-content id="scrollableNone"
                         scrollable="">
                <text class="textFont">First</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            特有属性 -- tid
        </text>
        <tabs class = "mode">
            <tab-content class="propNewAdd"
                     tid="id">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>
        <tabs class = "mode">
            <tab-content class="propNewAdd"
                     tid="">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>

        <text class="sub-title">
            特有属性 -- elif
        </text>
        <tabs class = "mode">
            <tab-content class="propNewAdd"
                         if="false">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>
        <tabs class = "mode">
            <tab-content class="propNewAdd"
                     elif="true">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>
        <tabs class = "mode">
            <tab-content class="propNewAdd"
                     elif="false">
                <text class="tab-text">Home</text>
                <text class="tab-text">Index</text>
            </tab-content>
        </tabs>

    </div>
</div>
