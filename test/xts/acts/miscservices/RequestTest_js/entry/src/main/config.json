{
	"app": {
		"bundleName": "com.example.myapplication.hmservice",
		"vendor": "example",
		"version": {
			"code": 1,
			"name": "1.0.0"
		},
		"apiVersion": {
			"compatible": 6,
			"target": 6,
			"releaseType": "Beta1"
		}
	},
