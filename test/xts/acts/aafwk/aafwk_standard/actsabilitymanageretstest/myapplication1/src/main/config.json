{
  "app": {
    "bundleName": "com.ohos.acecollaboration",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 7,
      "releaseType": "Release",
      "target": 7
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.myapplication1",
    "name": ".MyApplication",
    "mainAbility": ".MainAbility",
