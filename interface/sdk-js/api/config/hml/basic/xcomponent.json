      "name": "id",
      "required": true,
      "description": [
        {
          "name": "Unique ID of a component."
        }
      ],
      "type": "string"
    },
    {
      "name": "ref",
      "required": false,
      "description": [
        {
          "name": "Used to register reference information for elements or child components. The reference information is registered with the parent component on $refs."
        }
      ],
      "type": "string"
    },
    {
      "name": "type",
      "required": true,
      "description": [
        {
          "name": "Used to specify the xcomponent surface type."
        }
      ],
      "type": "enum",
      "options": [
        {
          "name": "surface"
        }
      ]
    },
    {
      "name": "libraryname",
      "required": false,
      "description": [
        {
          "name": "Used to specify the library file name of napi so."
        }
      ],
      "type": "string"
    }
