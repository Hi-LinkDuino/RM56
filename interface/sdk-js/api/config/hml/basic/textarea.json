      "required": false,
      "description": [
        {
          "name": "Indicates whether to enable the count subscript function for the text box. This parameter must be used together with maxlength."
        }
      ],
      "type": "enum",
      "options": [
        {
          "name": "false"
        },
        {
          "name": "true"
