<div id="container">
  <text id="testTitle">测试ID/Class选择器及内联样式</text>
  <text
    class="title"
    onclick="changeTitle"
    style="height:50px; color:{{textColor}}"
  >
    Hello {{title}}
  </text>
  <text
    class="title2"
    onclick="changeTitle2"
    style="height:50px; color:{{text2Color}}"
  >
    {{title}} {{title2}}
  </text>
  <div id="secDiv">
    <input class="button" type="button" value="返回首页" onclick="exitPage" />
  </div>
</div>