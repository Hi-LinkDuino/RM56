<div class="container" style="flex-direction:column; align-items:center">
    <div style="margin-top:40px;width:380px;height:80px;" class="divFrame">
        <text style="width:380px;height:80px;text-overflow:ellipsis">Component_chart_AttributeSet_Test_029</text>
    </div>
    <div style="height:220px;flex-direction:column;" class="divFrame">
        <div style="width:454px;height:50px">
            <text style="width:250px;height:50px">head size:-1</text>
            <chart style="width:200px;height:50px" type="line" style="left:0px; top:0px;" ref="chart1" datasets="{{datasets}}" options="{{options1}}">
            </chart>
        </div>
        <div style="width:454px;height:50px">
            <text style="width:250px;height:50px">bottom shape: aaa size:3 strWid:5</text>
            <chart style="width:200px;height:50px" type="line" style="left:0px; top:0px;" ref="chart2" datasets="{{datasets}}" options="{{options2}}">
            </chart>
        </div>
        <div style="width:454px;height:100px;">
            <text style="width:250px;height:100px;text-overflow:ellipsis">top strWid:5 strCol:0x0000ff fillCol:0x00ff00</text>
            <chart style="width:200px;height:50px" type="line" style="left:0px; top:0px;" ref="chart3" datasets="{{datasets}}" options="{{options3}}">
            </chart>
        </div>
    </div>
    <div class="divFrame">
        <input type="button" value="上页" style="width:90px;height:40px;" onclick="changePrev"/>
        <input type="button" value="首页" style="width:90px;height:40px;" onclick="backMain"/>
        <input type="button" value="下页" style="width:90px;height:40px;" onclick="changeNext"/>
    </div>
</div>