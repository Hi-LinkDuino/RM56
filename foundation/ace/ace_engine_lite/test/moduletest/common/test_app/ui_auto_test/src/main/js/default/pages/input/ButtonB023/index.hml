<div class = "container">
    <input type = "button" value = "{{value}}" class = "button" style ="fontSize:{{fontSize}};fontFamily:{{fontFamily}};color:{{color}}"/>
    <input type="button" value = "value" onclick="changeValue" class = "button"/>
    <input type = "button" value = "fontSize" onclick = "changeFontSize" class = "button"/>
    <input type = "button" value = "fontFamily" onclick = "changeFontFamily" class = "button"/>
    <input type = "button" value = "color" onclick = "changeColor" class = "button"/>
    <div class = "bottom">
        <input type = "button" value = "上一页" class = "pagePrevious" onclick="goPrevious"/>
        <input type = "button" value = "首页" class = "page" onclick="goHome"/>
        <input type = "button" value = "下一页" class = "page" onclick = "goNext"/>
    </div>
</div>