    <div class="container" style="flex-direction:column; align-items:center">
        <div style="margin-top:40px;width:320px;height:80px;" class="divFrame">
            <text style="width:320px;height:80px;text-overflow:ellipsis">Component_Div_StyleSet_Test_035</text>
        </div>
        <div style="height:220px;" class="divFrame">
            <div style="width:60px;height:180px;"></div>
            <div style="width:350px;height:180px;flex-direction:{{flexDirection}};justify-content:{{justifyContent}};align-items:{{alignItems}}">
                    <text style="width:320px;height:50px;color:#ffa500;">this is text</text>
                    <input type="button" style="width: 140px;height:40px" value="button"></input>
                    <stack class="stackBlock" style="border-color:#ffd700;background-color:#fa8072;">
                        <list style="left:0px;top:0px;width:150px;height:50px">
                            <list-item style="width: 150px; height:30px;">
                                <text style="left:0px;top:0px;width:150px;height:30px">listItem</text>
                            </list-item>
                            <list-item style="width: 150px; height:30px;">
                                <text style="left:0px;top:30px;width:150px;height:30px">listItem</text>
                            </list-item>
                        </list>
                    </stack>
                    <input type="checkbox" style="width: 50px;height:50px"> </input>
                    <stack class="stackBlock" style="border-color:#ffd700;background-color:#ffefd5;">
                        <div style="left:0px;top:0px;width:100px;height:100px;background-color:#00ff00;">
                            <image src="common/leftHead.png"></image>
                        </div>
                    </stack>
            </div>
        </div>
        <div style="width:454px;height:40px;justify-content:center">
            <input type="button" value="direction" style="width:120px;height:40px" onclick="changeFlexDirection"></input>
            <input type="button" value="justifyContent" style="width:120px;height:40px" onclick="changeJustifyContent"></input>
            <input type="button" value="alignItems" style="width:120px;height:40px" onclick="changeAlignItems"></input>
        </div>
        <div class="divFrame">
            <input type="button" value="上页" style="width:90px;height:40px;" onclick="changePrev"/>
            <input type="button" value="首页" style="width:90px;height:40px;" onclick="backMain"/>
            <input type="button" value="下页" style="width:90px;height:40px;" onclick="changeNext"/>
        </div>
    </div>