<stack id="div" style="width:454px;height:454px;">
    <swiper
        style="width:300px;height:250px;left:75px;top:75px;"
        index="{{indexVal}}"
        onclick="changeIf"
        onlongpress="changeIndex"
    >
        <stack style="left:0px;top:0px;width:454px;height:454px;background-color:#00ff00;" for="{{listData0}}">
            <text
                style="width:220px;height:60px;left:150px;"
            >
              第{{$item}}页
            </text>
            <text style="width:220px;height:60px;top:80px;left:150px;">
              children002
            </text>
        </stack>
        <stack style="left:0px;top:0px;width:454px;height:454px;background-color:#00ff00;" for="{{listData1}}">
            <text
                style="width:220px;height:60px;left:150px;"
            >
              第{{$item}}页
            </text>
        </stack>
        <stack if="{{ifStatus}}" style="left:0px;top:0px;width:454px;height:454px;background-color:#00ff00;">
            <text
                style="width:220px;height:60px;left:150px;"
            >
              第7页
            </text>
        </stack>
    </swiper>
    <input type="button" value="上页" style="width:90px;height:40px;left:80px;top:340px;" onclick="changePrev"/>
    <input type="button" value="首页" style="width:90px;height:40px;left:180px;top:340px;" onclick="backMain"/>
    <input type="button" value="下页" style="width:90px;height:40px;left:280px;top:340px;" onclick="changeNext"/>
</stack>
