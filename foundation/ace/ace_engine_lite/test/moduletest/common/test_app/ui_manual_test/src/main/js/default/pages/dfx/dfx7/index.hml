<stack
  style="width: 444px;height: 444px;margin: 20px;border-width: 5px;border-color: #00ff00;border-radius: 10px;"
>
  <text id="textid" style="left:100px;top:20px;width:400px;height:50px"
    >screensnap</text
  >
  <div class="commonStyle" onclick="makeScreenshot"></div>
  <text style="left:70px;top:320px;width:400px;height:40px">{{msg}}</text>
  <input
    type="button"
    value="上页"
    style="width:90px;height:40px;left:80px;top:380px;"
    onclick="changePrev"
  />
  <input
    type="button"
    value="首页"
    style="width:90px;height:40px;left:180px;top:380px;"
    onclick="backMain"
  />
  <input
    type="button"
    value="下页"
    style="width:90px;height:40px;left:280px;top:380px;"
    onclick="changeNext"
  />
</stack>