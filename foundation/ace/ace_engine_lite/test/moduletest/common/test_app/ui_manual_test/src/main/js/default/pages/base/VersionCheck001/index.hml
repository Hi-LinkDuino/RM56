<div id="container">
  <text id="testTitle">显示框架版本信息</text>
  <text class="title" style="height:50px; color:{{textColor}}">
    aceVersion: {{versionStr}}
  </text>
  <text class="title" style="height:50px; color:{{text2Color}}">
    commit: {{commitStr}}
  </text>
  <text class="title" style="height:50px; color:{{text2Color}}">
    {{buildStamp}}
  </text>
  <div id="secDiv">
    <input class="button" type="button" value="下一页" onclick="nextPage" />
    <div style="width: 20px; height: 20px"></div>
    <input class="button" type="button" value="返回首页" onclick="exitPage" />
  </div>
</div>