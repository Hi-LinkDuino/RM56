<stack id="div" style="width:454px;height:454px;">
    <swiper
        duration="{{duration}}"
        style="width:300px;height:300px;left:75px;top:75px;"
        vertical="false"
        onchange="showPage"
        onclick="updateDuration"
    >
        <stack style="left:0px;top:0px;width:300px;height:300px;">
            <text
                style="width:220px;height:60px;"
            >
                第{{index}}页，共2页
            </text>
            <text style="width:220px;height:60px;top:80px;">
              attr012
            </text>
        </stack>
        <stack style="left:0px;top:0px;width:300px;height:300px;">
            <text
                style="width:220px;height:60px;"
            >
                第{{index}}页，共2页
            </text>
        </stack>
        <stack style="left:0px;top:0px;width:300px;height:300px;">
            <text
                style="width:220px;height:60px;"
            >
                第{{index}}页，共2页
            </text>
        </stack>
    </swiper>
    <input type="button" value="上页" style="width:90px;height:40px;left:80px;top:340px;" onclick="changePrev"/>
    <input type="button" value="首页" style="width:90px;height:40px;left:180px;top:340px;" onclick="backMain"/>
    <input type="button" value="下页" style="width:90px;height:40px;left:280px;top:340px;" onclick="changeNext"/>
</stack>