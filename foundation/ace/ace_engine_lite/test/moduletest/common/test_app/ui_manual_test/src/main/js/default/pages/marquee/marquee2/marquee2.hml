<stack style="width:454px;height:454px">
  <text style="left:170px;top:30px;width:300px;height:40px">marquee2</text>
  <marquee
    scrollamount="65535"
    style="color:#00ff00;left:50px;top:130px;width:256px;height:60px"
    >Hello JS, scrollamount is 65535.</marquee
  >
  <marquee
    scrollamount="65535"
    class="textStyle"
    style="left:50px;top:230px;width:256px;height:60px"
    >{{text}}</marquee
  >
  <input
    type="button"
    value="上页"
    style="width:90px;height:40px;left:80px;top:340px;"
    onclick="changePrev"
  />
  <input
    type="button"
    value="首页"
    style="width:90px;height:40px;left:180px;top:340px;"
    onclick="backMain"
  />
  <input
    type="button"
    value="下页"
    style="width:90px;height:40px;left:280px;top:340px;"
    onclick="changeNext"
  />
</stack>