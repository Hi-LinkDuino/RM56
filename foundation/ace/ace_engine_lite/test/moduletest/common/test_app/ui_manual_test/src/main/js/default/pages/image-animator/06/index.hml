<div class="container">
  <text class="title">duration: {{duration}}ms</text>
  <image-animator
    class="player"
    images="{{frames}}"
    duration="{{duration}}"
  />
  <div class="box">
    <input
      class="btn"
      type="button"
      value="QUICK"
      @click="quick"
    />
    <input
      class="btn"
      type="button"
      value="SLOW"
      @click="slow"
    />
    <input
      class="btn"
      type="button"
      value="SET 0"
      @click="set0"
    />
    <input
      class="btn"
      type="button"
      value="SET 1"
      @click="set1"
    />
    <input
      class="btn"
      type="button"
      value="SET A"
      @click="setA"
    />
    <input
      class="btn"
      type="button"
      value="HOME"
      @click="goHome"
    />
  </div>
</div>
