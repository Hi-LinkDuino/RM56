<stack class="container">
    <div style="width: 700px; height: 50px; left: 20px;">
        <input onclick="changeStyle" style="width: 150px; height: 50px;" value="修改样式"></input>
        <input onclick="changeValue" style="width: 150px; height: 50px;" value="修改值"></input>
        <input onclick="invalidColor" style="width: 150px; height: 50px;" value="异常颜色"></input>
        <input onclick="invalidSize" style="width: 150px; height: 50px;" value="异常大小"></input>
        <input type="button" style="width:80px;height:50px;" value="首页" @click="backMain"/>
    </div>
    <qrcode onclick="clickAction" onswipe="swipeAction" style="width: {{width}}; height: {{height}}; top:{{top}}; left:{{left}}; border-width:{{borderWidth}}; border-color: red; color: {{color}}; background-color: {{bgcolor}};" value="{{qrValue}}">
    </qrcode>
</stack>
