<stack class="stack">
  <stack style="width: 454px; height: 80px; top: 80px">
    <input type="button" style="left: 85px; width: 130px; height: 60px" value="首页" onclick="backMain" />
    <input type="button" style="left: 235px; width: 130px; height: 60px" value="上一页" onclick="backSuperior" />
  </stack>
  <canvas
    ref="canvas1"
    style="width: 280px; height: 80px; top: 170px; left: 85px; background-color: #00ff00"
    onclick="clickFunc"
    onlongpress="longPressFunc"
    onswipe="swipeFunc"
  >
  </canvas>
  <text style="left: 40px; height: 40px; width: 400px; top: 260px; color: #ffa500">
    {{ text1 }}
  </text>
  <text class="text" style="left: 40px; width: 400px; top: 310px"> 矩形为canvas组件 </text>
  <text class="text" style="left: 40px; width: 400px; top: 360px"> 点击/长按/滑动canvas内部任意位置触发事件 </text>
</stack>
